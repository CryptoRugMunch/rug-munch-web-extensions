{
  "name": "rug-munch-extension",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:firefox": "npm run build:chrome && node scripts/build-firefox.cjs",
    "preview": "vite preview",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "build:chrome": "tsc -b && vite build",
    "build:all": "npm run build && npm run build:firefox && npm run build:safari",
    "build:safari": "node scripts/build-safari.cjs",
    "archive:safari": "node scripts/archive-safari.cjs",
    "archive:macos": "node scripts/archive-safari.cjs --macos-only",
    "archive:ios": "node scripts/archive-safari.cjs --ios-only",
    "archive:upload": "node scripts/archive-safari.cjs --upload",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@json-render/core": "^0.6.1",
    "@json-render/react": "^0.6.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.30",
    "@types/chrome": "^0.0.314",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "vite": "^6.3.5",
    "vitest": "^4.0.18"
  }
}